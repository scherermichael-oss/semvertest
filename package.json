{
  "name": "semvertest",
  "version": "0.0.0",
  "description": "Test for automatic semver releases",
  "main": "lib/index.js",
  "scripts": {
    "test": "bot",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/scherermichael/semvertest.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/scherermichael/semvertest/issues"
  },
  "homepage": "https://github.com/scherermichael/semvertest#readme",
  "devDependencies": {
    "assertthat": "1.0.0",
    "roboter": "0.16.0",
    "roboter-server": "0.16.0",
    "semantic-release": "^12.4.1"
  },
  "release": {
    "verifyConditions": ["@semantic-release/github"],
    "publish": ["@semantic-release/github"],
    "generateNotes": {
      "preset": "angular",
      "parserOpts": {
        "noteKeywords": ["BREAKING CHANGE", "BREAKING CHANGES", "BREAKING"]
      },
      "writerOpts": {
        "commitsSort": ["subject", "scope"]
      }
    }
  }
}
